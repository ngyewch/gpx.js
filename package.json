{
  "name": "@ngyewch/gpx.js",
  "version": "0.1.0",
  "description": "GPX parser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngyewch/gpx.js.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/gpx.js",
      "require": "./dist/gpx.umd.cjs",
      "script": "./dist/gpx.umd.cjs",
      "types": "./dist/gpx.d.ts"
    }
  },
  "module": "./dist/gpx.js",
  "main": "./dist/gpx.umd.cjs",
  "types": "./dist/gpx.d.ts",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "postbuild": "validate-package-exports --check --verify",
    "prepublishOnly": "validate-package-exports --check --verify",
    "test": "tap",
    "lint": "eslint src/ test/"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@tapjs/core": "4.0.0",
    "eslint": "9.10.0",
    "eslint-plugin-jsdoc": "50.2.2",
    "globals": "15.9.0",
    "tap": "21.0.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0",
    "validate-package-exports": "0.6.1",
    "vite": "5.4.4",
    "vite-plugin-dts": "4.2.1"
  },
  "dependencies": {
    "@xmldom/xmldom": "0.9.2",
    "date-fns": "3.6.0"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
